{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "https://tabletime-h51q.onrender.com/api/:match*"
    },
    {
      "source": "/:path*",
      "destination": "/"
    }
  ]
}
